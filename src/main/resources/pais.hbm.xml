<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.diana.model.Pais" table="PAIS">
         <id name="id" type="int" column="ID_PAIS">
        </id>
        <property name="nombrePais" type="string">
            <column name="NOMBRE_PAIS" length="64" not-null="true" />
        </property>
        <property name="codigoPais" type="string">
            <column name="CODIGO_PAIS" length="2" not-null="true"/>
        </property>
        <property name="valorPais" type="int">
            <column name="VALOR_PAIS" length="11" not-null="true"/>
        </property>
        
    </class>
</hibernate-mapping>